<%= stylesheet_link_tag 'gloable' %>
<% if flash.present? %>
<div class="container-fluid wzh-alert-top">
  <div class="row">
    <!--key: success / alert / error  -->
    <% flash.each do |key,value| %>
    <div class="col-lg-12 col-<%=key%>">
      <div class="message-<%=key%>"><%= value %></div>
    </div>
    <% end %>
  </div>

</div>
<% end %>